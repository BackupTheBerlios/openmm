dist: 
	@mkdir -p .tmp/svdrp && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/svdrp/ && ( cd `dirname .tmp/svdrp` && $(TAR) svdrp.tar svdrp && $(GZIP) svdrp.tar ) && $(MOVE) `dirname .tmp/svdrp`/svdrp.tar.gz . && $(DEL_FILE) -r .tmp/svdrp

